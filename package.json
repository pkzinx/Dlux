{
  "name": "dlux-barber",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "generate:atom": "yarn plop --plopfile ./generators/plopfile.js atoms",
    "generate:mole": "yarn plop --plopfile ./generators/plopfile.js molecules",
    "generate:orgs": "yarn plop --plopfile ./generators/plopfile.js organisms",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@styled-icons/bootstrap": "^10.34.0",
    "axios": "^1.6.0",
    "datocms-client": "^3.5.9",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "hamburger-react": "^2.4.1",
    "leaflet": "^1.9.4",
    "next": "^15.0.3",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-responsive": "^9.0.2",
    "react-scroll": "^1.9.0",
    "react-slick": "^0.29.0",
    "styled-components": "^5.3.11",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-typescript": "^7.22.15",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.8",
    "@types/leaflet": "^1.9.8",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-scroll": "^1.8.3",
    "@types/react-slick": "^0.23.13",
    "@types/styled-components": "^5.1.32",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vercel/node": "^3.0.7",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.54.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^15.1.0",
    "plop": "^4.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  }
}
